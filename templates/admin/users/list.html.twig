{% extends 'base.html.twig' %}

{% block title %}User list{% endblock %}

{% block content %}
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <ul>
                    {% for user in users %}
                        <li>{{ user.email }}</li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{% endblock %}
